{
  "name": "moment-parseplus",
  "version": "2.0.2",
  "description": "Date parsing plugin for momentjs",
  "type": "module",
  "main": "parseplus.mjs",
  "exports": {
    "import": "./parseplus.mjs",
    "require": "./parseplus.cjs"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "sh ./test/run-jest.sh",
    "watch": "sh ./test/run-jest.sh --watch",
    "coverage": "sh ./test/run-jest.sh --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kensnyder/moment-parseplus.git"
  },
  "keywords": [
    "moment",
    "momentjs",
    "date",
    "parse",
    "parsing",
    "locale"
  ],
  "author": "Ken Snyder <kendsnyder@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kensnyder/moment-parseplus/issues"
  },
  "homepage": "https://github.com/kensnyder/moment-parseplus#readme",
  "dependencies": {
    "any-date-parser": "1.5.3",
    "moment": "^2.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "jest": "29.1.2",
    "prettier": "2.7.1"
  },
  "jest": {
    "transform": {
      "^.+\\.mjs$": "babel-jest"
    },
    "testMatch": [
      "**/*.spec.mjs"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  }
}
